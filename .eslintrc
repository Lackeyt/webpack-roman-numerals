{
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "node": true
  },
  "rules": {
    "semi": 1,
    "indent": ["warn", 2],
    "no-console": "warn",
    "no-debugger": "warn"
  },
  "globals": {
    "describe": "readonly",
    "test": "readonly",
    "expect": "readonly"
  }
}